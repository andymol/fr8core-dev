@{
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@Html.Partial("~/Views/Account/_HeaderPartial.cshtml")

<div class="registration-section  col-md-4 col-md-offset-3">
    <div class="wrapper">
        <h2 class="section-title with-border">Thanks for signing up!</h2>
        <div class="row">
            <div class="col-md-12">
                <div class="custom-form">
                    <p>Taking you now to the @Html.ActionLink("Login page", "Index", "Account", null, new { @class = "" })</p>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/js/jqueryvalidate")
    @Scripts.Render("~/bundles/js/customjs")
}

<script type="text/javascript">
    $(document).ready(function () {
        $('#dashboardLink').empty();
    });

</script>
